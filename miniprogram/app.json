{
  "permission": {
    "scope.userLocation": {
      "desc": "你的位置信息将用于小程序位置接口的效果展示"
    }
  },
  "pages": [
    "pages/more/more",
    "pages/login/login",
    "pages/myself/myself"
  ],
  "subPackages": [
    {
      "name": "more",
      "root": "pages/more/pages",
      "pages": [
        "DetailContent/DetailContent",
        "saveBureau/saveBureau",
        "UserContent/UserContent",
        "NewInfo/NewInfo",
        "PublishContent/PublishContent",
        "Freshman/Freshman",
        "Match/Match",
        "LoseDetailContent/LoseDetailContent",
        "Game/Game",
        "UserCenter/index",
        "saveBureau/publishBureau/publishBureau",
        "saveBureau/bureauContent/bureauContent",
        "saveBureau/joinedBureau/joinedBureau"
      ]
    },
    {
      "name": "about",
      "root": "pages/about",
      "pages": [
        "about"
      ]
    },
    {
      "name": "journal",
      "root": "pages/journal",
      "pages": [
        "journal"
      ]
    }
  ],
  "preloadRule": {
    "pages/more/more": {
      "network": "all",
      "packages": [
        "more"
      ]
    },
    "pages/myself/myself": {
      "network": "all",
      "packages": [
        "about",
        "journal"
      ]
    }
  },
  "window": {
    "navigationBarTitleText": "We校园",
    "backgroundColor": "@backgroundColor",
    "backgroundTextStyle": "@backgroundTextStyle",
    "navigationBarBackgroundColor": "@navBackgroundColor",
    "navigationBarTextStyle": "@navTextStyle"
  },
  "tabBar": {
    "color": "@color",
    "selectedColor": "#6eaddf",
    "backgroundColor": "@tabBackgroundColor",
    "list": [
      {
        "pagePath": "pages/more/more",
        "iconPath": "/images/tabbar/圈子.png",
        "selectedIconPath": "/images/tabbar/圈子_sel.png",
        "text": "校园圈"
      },
      {
        "pagePath": "pages/myself/myself",
        "iconPath": "/images/tabbar/作者.png",
        "selectedIconPath": "/images/tabbar/作者_sel.png",
        "text": "我的"
      }
    ]
  },
  "sitemapLocation": "sitemap.json",
  "themeLocation": "theme.json",
  "darkmode": true,
  "style": "v2"
}